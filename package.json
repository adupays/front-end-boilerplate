{
  "name": "front-end-boilerplate",
  "version": "1.0.0",
  "description": "Front-End Boilerplate",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "prod-css": "./node_modules/.bin/gulp prod",
    "dev-css": "./node_modules/.bin/gulp dev",
    "watch": "./node_modules/.bin/gulp watch",
    "watch-dev-js": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js --progress --colors --hot --inline --devtool=eval",
    "build-dev-js": "./node_modules/.bin/webpack --config ./webpack.config.js -d --progress --colors",
    "watch-prod-js": "./node_modules/.bin/webpack --config ./webpack.config.js -p --watch",
    "build-prod-js": "./node_modules/.bin/webpack --config ./webpack.config.js -p",
    "eslint": "eslint -c .eslintrc ./src/js/"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "chokidar": "^1.5.0",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "estraverse-fb": "^1.3.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.7",
    "gulp-concat": "^2.6.0",
    "gulp-less": "^3.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "merge2": "^1.0.2",
    "moment": "^2.13.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "draft-js": "^0.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.3",
    "lodash": "^4.12.0",
    "react": "^0.14.8",
    "react-bootstrap": "^0.28.5",
    "react-bootstrap-switch": "^3.4.5",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
